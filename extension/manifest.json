{
  "manifest_version": 3,
  "name": "Yahudim App Extension",
  "version": "1.0.1",
  "description": "A Chrome extension for Yahudim App.",
  "action": {
    "default_popup": "popup.html"
  },
  "permissions": [],
  "host_permissions": [
    "http://localhost:3000/*",
    "https://yahudim.app/*"
  ],
  "content_security_policy": {
    "extension_pages": "script-src 'self' 'wasm-unsafe-eval'; object-src 'self'"
  }
}
